{
  "name": "apollo-graphql-simple-server",
  "packageManager": "yarn@4.5.3",
  "type": "module",
  "scripts": {
    "compile": "tsc && tsc-alias",
    "start": "yarn compile && node ./dist/index.js",
    "dev": "nodemon --watch src --ext ts,graphql --exec \"yarn compile && node ./dist/index.js\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@apollo/server": "^4.11.2",
    "graphql": "^16.10.0",
    "sequelize": "^6.37.5",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "sequelize-cli": "^6.6.2",
    "ts-jest": "^29.2.5",
    "tsc-alias": "1.8.10",
    "typescript": "^5.7.2"
  }
}
